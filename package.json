{
  "name": "cosap_frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@cyntler/react-doc-viewer": "^1.14.0",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@fontsource/poppins": "^4.5.2",
    "@mui/icons-material": "^5.3.1",
    "@mui/material": "^5.4.0",
    "@mui/x-data-grid": "^5.6.0",
    "@mui/x-data-grid-generator": "^5.6.0",
    "@sentry/react": "^7.52.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^0.27.2",
    "chart.js": "^3.7.1",
    "chonky": "^2.3.2",
    "chonky-icon-fontawesome": "^2.3.2",
    "eslint-plugin-import": "^2.25.4",
    "filepond": "^4.30.3",
    "formik": "^2.2.9",
    "igv": "^2.13.10",
    "js-base64": "^3.7.5",
    "js-cookie": "^3.0.1",
    "json-server": "^0.17.0",
    "jwt-decode": "^3.1.2",
    "nanoid": "^4.0.0",
    "react": "^17.0.2",
    "react-chartjs-2": "^4.0.1",
    "react-dom": "^17.0.2",
    "react-filepond": "^7.1.1",
    "react-multi-carousel": "^2.8.2",
    "react-pdf": "^5.7.1",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.2.1",
    "react-scripts": "^5.0.0",
    "react-tsparticles": "^2.9.3",
    "recharts": "^2.1.9",
    "stream": "^0.0.2",
    "streamsaver": "^2.0.6",
    "tsparticles": "^2.9.3",
    "tsparticles-path-polygon": "^2.9.3",
    "tsparticles-plugin-canvas-mask": "^2.9.3",
    "tsparticles-plugin-polygon-mask": "^2.9.3",
    "web-streams-polyfill": "^3.2.1",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11",
    "zustand": "^4.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint --fix --ext .js,.jsx ."
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:import/recommended",
      "google",
      "prettier"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "react",
      "import",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "react/prop-types": "off",
      "require-jsdoc": 0,
      "sort-imports": [
        "error",
        {
          "ignoreCase": true,
          "ignoreDeclarationSort": true
        }
      ],
      "import/order": [
        1,
        {
          "groups": [
            "external",
            "builtin",
            "internal",
            "sibling",
            "parent",
            "index"
          ]
        }
      ]
    },
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "eslint": "^8.11.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "prettier": "2.5.1",
    "react-error-overlay": "^6.0.9"
  }
}
